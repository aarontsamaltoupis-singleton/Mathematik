{
  "@jupyterlab/terminal-extension:plugin": {
    "theme": "dark"
  },
  "@jupyterlab/notebook-extension:tracker": {
    "codeCellConfig": {
      "lineNumbers": false,
      "lineWrap": false,
      "autoClosingBrackets": true
    },
    "markdownCellConfig": {
      "lineNumbers": false,
      "matchBrackets": false,
      "autoClosingBrackets": true
    },
    "rawCellConfig": {
      "lineNumbers": false,
      "matchBrackets": false,
      "autoClosingBrackets": true
    }
  },
  "@jupyterlab/apputils-extension:notification": {
    "fetchNews": "true"
  },
  "@jupyterlab/lsp-extension:plugin": {
    "activate": "on",
    "languageServers": {
      "pylsp": {
        "configuration": {},
        "rank": 50
      }
    },
    "logAllCommunication": false,
    "setTrace": "off"
  },
  "@jupyterlab/fileeditor-extension:plugin": {
    "editorConfig": {
      "lineNumbers": true,
      "autoClosingBrackets": true
    }
  },
  "@jupyterlab/console-extension:tracker": {
    "promptCellConfig": {
      "codeFolding": false,
      "lineNumbers": false,
      "autoClosingBrackets": true
    }
  },
  "@jupyterlab/completer-extension:manager": {
    "availableProviders": {
      "CompletionProvider:context": 500,
      "CompletionProvider:kernel": 550,
      "DebuggerCompletionProvider": 1000
    },
    "providerTimeout": 1000,
    "showDocumentationPanel": false,
    "autoCompletion": true,
    "suppressIfInlineCompleterActive": true
  }
}